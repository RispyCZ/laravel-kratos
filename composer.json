{
    "name": "chivincent/laravel-kratos",
    "description": "Laravel Kratos integrates authentication features with ory/kratos",
    "type": "library",
    "keywords": ["laravel", "auth", "authentication", "kratos", "ory-kratos"],
    "require": {
        "php": "^8.4||^8.5",
        "ory/kratos-client-php": "^v25.4.0",
        "illuminate/support": "^12"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.22",
        "phpunit/phpunit": "^12",
        "orchestra/testbench": "^10"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Chivincent\\LaravelKratos\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests"
        }
    },
    "authors": [
        {
            "name": "Vincent Chi",
            "email": "song374561@chivincent.net"
        }
    ],
    "minimum-stability": "stable",
    "scripts": {
        "test": "vendor/bin/phpunit",
        "lint": "vendor/bin/php-cs-fixer"
    },
    "extra": {
        "laravel": {
            "providers": [
            "Chivincent\\LaravelKratos\\KratosServiceProvider"
            ]
        }
    }
}
